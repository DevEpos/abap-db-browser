     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Field value comparisons in &lt;DS:TX.DDDOK_SELEKTIONSBEDINGUNG&gt;selection</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>conditions&lt;/&gt; can be linked with each other through the logical</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>operators &apos;AND&apos; and &apos;OR&apos;. OR operators are only permitted between field</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>value comparisons that refer to the same field.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
